<% if(typeof(finalresult.description[0]) === 'undefined'){ %>
    <%- include("./partial/500") %>
<% }else{ %>
    <%- include("./partial/header") %>
    <button onclick="topFunction()" id="myBtn" title="Go to top"><i class='fas fa-angle-double-up'></i></button>

    <!-- Page Content -->
    <div class=" page-heading contact-heading header-text">
        <div class="container">
            <div class="row">
                <div class="col-md-12">
                    <div class="text-content">
                        <h4></h4>
                        <h2>
                            <%= companyName %>
                        </h2>
                    </div> 
                </div>
            </div>
        </div>
    </div>

    
    <div class="latest-products">
        <div class="container">
            <div class="row">
                <% for(i=0;i< finalresult.allProductOfCompany.length; i++ ){ %>
                <div class="col-md-4">
                    <div class="product-item">
                        <a href="/brand/<%= companyName %>/<%= finalresult.allProductOfCompany[i].product_name %>">
                            <img class="img-responsive" src="<%= finalresult.allProductOfCompany[i].product_img %>" alt="<%= finalresult.allProductOfCompany[i].product_name %>">
                        </a>
                        <div class="down-content">
                            <a href="/brand/<%= companyName %>/<%= finalresult.allProductOfCompany[i].product_name %>">
                                <h4><%= finalresult.allProductOfCompany[i].product_name %></h4>
                            </a>
                            <h6><i class="fas fa-rupee-sign"></i><%= finalresult.allProductOfCompany[i].product_price %></h6>
                        </div>
                        <button class="btn btn-block btn-success add-to-cart" 
                        id="<%= finalresult.allProductOfCompany[i].product_pkg_id %>" 
                        data-name="<%= finalresult.allProductOfCompany[i].product_name %>"
                        data-price="<%= finalresult.allProductOfCompany[i].product_price %>"
                        data-weight="<%= finalresult.allProductOfCompany[i].product_weight_gram %>"
                        data-size="<%= finalresult.allProductOfCompany[i].product_size %>"
                        data-img="<%= finalresult.allProductOfCompany[i].product_img %>"
                        value="<%= finalresult.allProductOfCompany[i].product_pkg_id %>" type="button" >Add to cart</button>
                    </div>
                </div>
                <% } %>
            </div>
        </div>
    </div>
    <div class="container">
        <div class="section-heading company-section">
            <h2>About <%= companyName %>
            </h2>
        </div>
        <div class="company-description">
            
                <%= finalresult.description[0].companyDescription %>
            
        </div>

    </div>
    <%- include("./partial/footer") %>
<% } %>
